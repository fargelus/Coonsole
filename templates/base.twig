<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        {% block meta %}
            <meta name="keywords" content="{% if meta is defined %}{{ meta }}{% endif %}" />
        {% endblock %}
        {% block javascripts %}
            <script type="text/javascript" defer="defer" src="bundle.js"></script>
        {% endblock %}
    </head>
    <body>
        {% block body %}
            <div id="app"></div>
            {% block header %}
                {% include 'header.twig' %}
            {% endblock %}

            {% block content %}
            {% endblock %}
        {% endblock %}
        {% block login %}
        {% endblock %}
    </body>
</html>
